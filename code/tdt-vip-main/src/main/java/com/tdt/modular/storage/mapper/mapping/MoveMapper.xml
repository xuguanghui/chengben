<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tdt.modular.storage.mapper.MoveMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.tdt.modular.storage.entity.Move">
        <id column="id" property="id" />
        <result column="moveno" property="moveno" />
        <result column="commodityid" property="commodityid" />
        <result column="commoditybar" property="commoditybar" />
        <result column="commodityname" property="commodityname" />
        <result column="locatorid" property="locatorid" />
        <result column="locatorcode" property="locatorcode" />
        <result column="locatorname" property="locatorname" />
        <result column="oldlocatorid" property="oldlocatorid" />
        <result column="oldlocatorcode" property="oldlocatorcode" />
        <result column="oldlocatorname" property="oldlocatorname" />
        <result column="qty" property="qty" />
        <result column="state" property="state" />
        <result column="warehouseid" property="warehouseid" />
        <result column="createid" property="createid" />
        <result column="creator" property="creator" />
        <result column="createtime" property="createtime" />
        <result column="updateid" property="updateid" />
        <result column="updator" property="updator" />
        <result column="updatetime" property="updatetime" />
        <result column="auditid" property="auditid" />
        <result column="auditor" property="auditor" />
        <result column="audittime" property="audittime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS "id", moveno AS "moveno", commodityid AS "commodityid", commoditybar AS "commoditybar", commodityname AS "commodityname", locatorid AS "locatorid", locatorcode AS "locatorcode", locatorname AS "locatorname", oldlocatorid AS "oldlocatorid", oldlocatorcode AS "oldlocatorcode", oldlocatorname AS "oldlocatorname", qty AS "qty", state AS "state", warehouseid AS "warehouseid", createid AS "createid", creator AS "creator", createtime AS "createtime", updateid AS "updateid", updator AS "updator", updatetime AS "updatetime", auditid AS "auditid", auditor AS "auditor", audittime AS "audittime"
    </sql>


    <select id="customList" resultType="com.tdt.modular.storage.model.result.MoveResult" parameterType="com.tdt.modular.storage.model.params.MoveParam">
        select
        <include refid="Base_Column_List"/>
        from s_move where 1 = 1
            <if test="paramCondition.state != null and paramCondition.state != ''">
                and state like CONCAT('%',#{paramCondition.state},'%')
            </if>
            <if test="paramCondition.commoditybar != null and paramCondition.commoditybar != ''">
                and commoditybar like CONCAT('%',#{paramCondition.commoditybar},'%')
            </if>
            <if test="paramCondition.commodityname != null and paramCondition.commodityname != ''">
                and commodityname like CONCAT('%',#{paramCondition.commodityname},'%')
            </if>
            <if test="paramCondition.locatorcode != null and paramCondition.locatorcode != ''">
                and locatorcode like CONCAT('%',#{paramCondition.locatorcode},'%')
            </if>
            <if test="paramCondition.locatorname != null and paramCondition.locatorname != ''">
                and locatorname like CONCAT('%',#{paramCondition.locatorname},'%')
            </if>
    </select>

    <select id="customMapList" resultType="map" parameterType="com.tdt.modular.storage.model.params.MoveParam">
        select
        <include refid="Base_Column_List"/>
        from s_move where 1 = 1 and warehouseid = #{paramCondition.warehouseid}
            <if test="paramCondition.state != null and paramCondition.state != ''">
                and state = #{state}
            </if>
            <if test="paramCondition.commoditybar != null and paramCondition.commoditybar != ''">
                and commoditybar like CONCAT('%',#{paramCondition.commoditybar},'%')
            </if>
            <if test="paramCondition.commodityname != null and paramCondition.commodityname != ''">
                and commodityname like CONCAT('%',#{paramCondition.commodityname},'%')
            </if>
            <if test="paramCondition.locatorcode != null and paramCondition.locatorcode != ''">
                and locatorcode like CONCAT('%',#{paramCondition.locatorcode},'%')
            </if>
            <if test="paramCondition.locatorname != null and paramCondition.locatorname != ''">
                and locatorname like CONCAT('%',#{paramCondition.locatorname},'%')
            </if>
    </select>

    <select id="customPageList" resultType="com.tdt.modular.storage.model.result.MoveResult" parameterType="com.tdt.modular.storage.model.params.MoveParam">
        select
        <include refid="Base_Column_List"/>
        from s_move where 1 = 1
            <if test="paramCondition.state != null and paramCondition.state != ''">
                and state like CONCAT('%',#{paramCondition.state},'%')
            </if>
            <if test="paramCondition.commoditybar != null and paramCondition.commoditybar != ''">
                and commoditybar like CONCAT('%',#{paramCondition.commoditybar},'%')
            </if>
            <if test="paramCondition.commodityname != null and paramCondition.commodityname != ''">
                and commodityname like CONCAT('%',#{paramCondition.commodityname},'%')
            </if>
            <if test="paramCondition.locatorcode != null and paramCondition.locatorcode != ''">
                and locatorcode like CONCAT('%',#{paramCondition.locatorcode},'%')
            </if>
            <if test="paramCondition.locatorname != null and paramCondition.locatorname != ''">
                and locatorname like CONCAT('%',#{paramCondition.locatorname},'%')
            </if>
    </select>

    <select id="customPageMapList" resultType="map" parameterType="com.tdt.modular.storage.model.params.MoveParam">
        select
        <include refid="Base_Column_List"/>
        from s_move where 1 = 1
            <if test="paramCondition.state != null and paramCondition.state != ''">
                and state like CONCAT('%',#{paramCondition.state},'%')
            </if>
            <if test="paramCondition.commoditybar != null and paramCondition.commoditybar != ''">
                and commoditybar like CONCAT('%',#{paramCondition.commoditybar},'%')
            </if>
            <if test="paramCondition.commodityname != null and paramCondition.commodityname != ''">
                and commodityname like CONCAT('%',#{paramCondition.commodityname},'%')
            </if>
            <if test="paramCondition.locatorcode != null and paramCondition.locatorcode != ''">
                and locatorcode like CONCAT('%',#{paramCondition.locatorcode},'%')
            </if>
            <if test="paramCondition.locatorname != null and paramCondition.locatorname != ''">
                and locatorname like CONCAT('%',#{paramCondition.locatorname},'%')
            </if>
    </select>

</mapper>
